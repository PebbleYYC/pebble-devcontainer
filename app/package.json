{
  "name": "departure-board",
  "author": "David Teresi",
  "version": "1.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "install": "^0.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-env": "^7.26.9",
    "ajv": "^8.17.1",
    "babel-loader": "^8.4.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.41.0",
    "eslint": "^9.24.0",
    "regenerator-runtime": "^0.13.11",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0",
    "whatwg-fetch": "^3.6.20"
  },
  "capabilities": [
    "location"
  ],
  "pebble": {
    "displayName": "Departure Board",
    "uuid": "a18a191e-51e0-48a3-9c63-075eea857333",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "basalt"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "num_routes",
      "time[12]",
      "unit[12]",
      "stop_name[12]",
      "dest_name[12]",
      "route_number[12]",
      "route_name[12]",
      "vehicle_type[12]",
      "color[12]",
      "shape[12]"
    ],
    "resources": {
      "media": [
        {
          "type": "raw",
          "name": "PLANE",
          "file": "Pebble_80x80_Scheduled_flight.pdc"
        },
        {
          "type": "raw",
          "name": "GENERIC_FAILED",
          "file": "Pebble_80x80_Generic_failed.pdc"
        },
        {
          "type": "raw",
          "name": "NO_INTERNET",
          "file": "Pebble_80x80_Check_internet_connection.pdc"
        },
        {
          "type": "raw",
          "name": "STREETCAR_ANIM",
          "file": "streetcar_frames.pdc"
        },
        {
          "type": "raw",
          "name": "SUBWAY_ANIM",
          "file": "srt_frames.pdc"
        },
        {
          "type": "raw",
          "name": "BUS_ANIM",
          "file": "bus_frames.pdc"
        },
        {
          "type": "raw",
          "name": "TRAIN_ANIM",
          "file": "train_frames.pdc"
        }
      ]
    }
  }
}
